<!--pages/sustc-bus.wxml-->
<div id="page">
  <h1>南科大班车</h1>
  <div class="first-line-bus line">
    <h2 class="line-name">欣园>>工学院</h2>
    <block wx:for="{{ line_1_time }}" wx:key="index">
      <view class="bus-line">{{ item }}</view>
    </block>
    <h2>工学院--欣园</h2>
    <block wx:for="{{ line_1_time_reverse }}" wx:key="index">
      <view>{{ item }}</view>
    </block>
  </div> 
  <div class="holiday-line-bus line">
    <h2>欣园--工学院</h2>
    <block wx:for="{{ line_1_holiday }}" wx:key="index">
      <view>{{ item }}</view>
    </block>
    <h2>工学院--欣园</h2>
    <block wx:for="{{ line_1_holiday_reverse }}" wx:key="index">
      <view>{{ item }}</view>
    </block>
  </div>
  <div class="second-line-bus line">
    <h2>欣园--科研楼</h2>
    <block wx:for="{{ line_2_time }}" wx:key="index">
      <view>{{ item }}</view>
    </block>
    <h2>科研楼--欣园</h2>
    <block wx:for="{{ line_2_time_reverse }}" wx:key="index">
      <view>{{ item }}</view>
    </block>
  </div>
  <div class="zhiyuan-line-bus line">
    <h2>智园--教师食堂</h2>
    <block wx:for="{{ line_Zhiyuan }}" wx:key="index">
      <view>{{ item }}</view>
    </block>
  </div>
</div>